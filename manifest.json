{
    "name": "Simple Youtube Downloader", 
    "description": "Download Youtube videos easier than ever!",
    "version": "0.1",
    "author": "Sam Sangeeth",
    "manifest_version": 2,
    "web_accessible_resources": ["src/youtubedl.js"],
    "content_scripts": [
                            {
                                "matches": ["https://www.youtube.com/watch*"],
                                "js": ["src/loader.js"]

                            }
                        ]
}